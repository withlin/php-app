{
    "family": "php-app",
    "containerDefinitions": [
        {
            "name": "php-app",
            "image": "619071339509.dkr.ecr.ap-northeast-1.amazonaws.com/devops/php-app:v0.0.3",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "LOG_CHANNEL",
                    "value": "stderr"
                },
                {
                    "name": "CUSTOM_ENV_VAR",
                    "value": "Hello from aws appconfig define Environment Variable!!!!!!!!!!"
                },
                {
                    "name": "ENV",
                    "value": "this is from appconfig"
                },
                {
                    "name": "APP_KEY",
                    "value": "base64:dckYPK3SqUk4cI0g0j6nZQNhICnJIEBZUzg0z2OoOsM="
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/php-app",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::619071339509:role/ecs-task-execution-role",
    "taskRoleArn": "arn:aws:iam::619071339509:role/ecs-task-role",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
